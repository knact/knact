<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<javafx.scene.layout.VBox styleClass="tool-bar" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox spacing="4.0">
            <children>
                <Button fx:id="add" mnemonicParsing="false" text="+" />
                <Button fx:id="delete" mnemonicParsing="false" text="-" />
                <TextField fx:id="filter" HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
    </children>
</javafx.scene.layout.VBox>
